                        <form action="/aplicacion/consultasAdi" method="POST">
                                    
							
								<fieldset>
									<legend>Seleccione un paciente activo:</legend>

									<div>
										{{#if ListaPaciente}}
										{{#each ListaPaciente}}
										{{#each this}}
										<input type="radio" id={{id}} name="idpaciente_id" value={{id}}
												checked>
										<label for={{id}}>{{Nombres}}-{{Apellidos}}</label>
										{{/each}}
										{{/each}}
										{{/if}}
									</div>
                                 								<fieldset>
									<legend>Seleccione una categoria:</legend>

									<div>
										{{#if pedo}}
										{{#each pedo}}
										{{#each this}}
										<input type="radio" id={{Descripcion}} name="TipoDiagnostico" value={{Descripcion}}
												checked>
										<label for={{Descripcion}}>{{Descripcion}}</label>
										{{/each}}
										{{/each}}
										{{/if}}
									</div>
                                 
								</fieldset>
								</fieldset>
									
									<fieldset>
									<legend>Seleccione un usuario activo:</legend>

									<div>
										{{#if caca}}
										{{#each caca}}
										{{#each this}}
										<input type="radio" id={{id}} name="idusuario_id" value={{id}}
												checked>
										<label for={{id}}>{{Nombres}}</label>
										{{/each}}
										{{/each}}
										{{/if}}
									</div>
                                 
								</fieldset>
                                <input type="text" name="Motivo" placeholder="motivo">
								<input type="text" name="Fecha" placeholder="fecha">
								<input type="text" name="Cantidad" placeholder=" cantidad ">
								
								<input type="submit" value="adicionar categorias ">
